%div
  %div
    %form{class: 'search_users_form left', action: admin_users_path(format: :json, per_page: 10), data: { remote: true }}
      %input{name: 'q[nick_name_or_mobile_contains]', type: 'search', id: 'input_search_users'}
      %input{name: 'submit', type: 'submit', value: '搜索', id: 'btn_search_users'}

  %table.users
    %thead
      %tr
        %th 用户ID
        %th 用户昵称
        %th 手机号
        %th 操作
    %tbody